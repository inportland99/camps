<!DOCTYPE html>
<html>
  <head>
    <title>Math Plus Summer Camps | Columbus Ohio's #1 Tech Camps</title>
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= render 'layouts/facebook_pixel_script' if Rails.env.production? && !current_user %>
    <%= javascript_include_tag "https://js.stripe.com/v1/", "application" %>
    <%= render 'layouts/gtm_head' if Rails.env.production? && !current_user %>
    <%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="google-site-verification" content="vgW06OQ-xxKcGshIrouAKNY2yvRl7KxGrS2KPW7UwFc" />
    <%= render 'layouts/facebook_meta_tags' %>

    <script>
    window.addEventListener("load", function(){
    window.cookieconsent.initialise({
      "palette": {
        "popup": {
          "background": "#edeff5",
          "text": "#838391"
        },
        "button": {
          "background": "#4b81e8"
        }
      },
      "content": {
        "message": "This website uses cookies to ensure you get the best experience on our website.",
        "dismiss": "Got it!",
        "link": "Learn more",
        "href": "http://cookies.insites.com/about-cookies"
      }
    })});
    </script>

  </head>
  <body>
    <%= render 'layouts/gtm_body' if Rails.env.production? && !current_user %>

    <%= render 'layouts/facebook_SDK' %>

    <%= render 'layouts/twitter_SDK' %>

    <%= render 'layouts/navbar' %>

    <%= flash_messages %>

    <%= yield %>

    <% if Rails.env.development? %>
      <div class="container" style= "margin-top:25px;">
        <div class="card">
          <div class="card-block">
            <%= debug(params) %>
          </div>
        </div>
      </div>
    <% end %>

    <hr>
    <%= render 'layouts/footer' %>

    <%# render 'layouts/ga' if Rails.env.production? %>
    <%= render '/pages/help_modal' %>
  </body>
</html>
